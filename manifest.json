
{
	"manifest_version": 2,
	"name":"Wikipedia Beatiful Maker",
	"description":"A wikipedia application to make it beatiful",
	"version":"1.0",

	  "browser_action": {
    "default_icon": "icon.png"
  },

			  "content_scripts": [{
    "css": ["style.css"],
    "js": ["content.js"],
    "matches": ["https://fr.wikipedia.org/*"]
  }]

}


